{
  "compilerOptions": {
    "target": "ES5",
    "jsx": "react-native",
    "lib": ["ES2015", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
